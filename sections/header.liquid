{% case section.settings.header_type %}
  <!-- *****======HEADER STYLE 1 ======****** -->
  {% when 'header1' %}

  <link rel="stylesheet" href="{{ 'component-cart-notification.css' | asset_url }}" media="print" onload="this.media='all'">
  
  <noscript>{{ 'component-cart-notification.css' | asset_url | stylesheet_tag }}</noscript>
  
  <script src="{{ 'details-disclosure.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'details-modal.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'cart-notification.js' | asset_url }}" defer="defer"></script>

  <header class="header header1 header1--solid">
    <div class="header1__wrapper py-3">
      <div class="container">
        <div class="row mx-auto">
          <div class="col-4 col-md-4 d-flex align-items-center justify-content-start">
            <nav class="header1__nav-left">
              <ul class="header1__nav-left-list d-flex align-items-center">
                {%- if section.settings.menu != blank -%}
                  {%- for link in section.settings.menu.links -%}
                    {%- if settings.mega_title == link.title -%}
                      <!-- REGULAR LINK WITH A DROPDOWN MEGAMENU LEFT SIDE -->
                      <li class="header1__nav-left-list-item header1__has-dropdown site-nav--is-megamenu">
                        <a href="" class="header1__nav-left-link header1__nav--link-has-dropdown">
                          {{ link.title | escape }} <span className="dropdown-icon"><i class="bi bi-chevron-down"></i></span>
                        </a>
                        <!-- Megamenu START -->
                        <div class="nav-dropdown text-left megamenu">
                          <div class="page-width">
                            <div class="container">
                              <div class="row">
                                {% for childlink in link.links %}
                                  <a href="#" class="megamenu__collection-img"></a>
                                  <div class="col-sm-3 col-md-3 col-lg-3">
                                    <h5 class="megamenu__collection-heading py-3">
                                      <a href="#">{{ childlink.title }}</a>
                                    </h5>
                                    <ul class="megamenu__list text-left nav-dropdown__list">
                                      {% for grandchildlink in childlink.links %}
                                        <li class="nav-dropdown__list-item">
                                          <a href="{{ grandchildlink.url }}" class="nav-dropdown__link">
                                            {{ grandchildlink.title }}
                                          </a>
                                        </li>
                                      {%endfor%}
                                    </ul>
                                  </div>
                                {% endfor %}
                              </div>
                            </div>
                          </div>
                        </div>
                        <!-- Megamenu END -->
                      </li>

                    {%- elsif link.links != blank -%}
                      <!-- REGULAR LINK WITH A DROPDOWN MENU LEFT SIDE -->
                      <li class="header1__nav-left-list-item header1__has-dropdown">
                        <a href="" class="header1__nav-left-link header1__nav--link-has-dropdown">
                          {{ link.title | escape }} <span class="dropdown-icon"><i class="bi bi-chevron-down"></i></span>
                        </a>
                        <!-- Dropdown Menu START -->
                        <ul class="nav-dropdown text-left nav-dropdown__list">
                          {% for childlink in link.links %}
                            <li class="nav-dropdown__list-item">
                              <a href="{{ childlink.url }}" class="nav-dropdown__link">{{ childlink.title | escape }}</a>
                            </li>
                          {% endfor %}
                        </ul>
                        <!-- Dropdown Menu END -->
                      </li>
                    {%- else -%}
                      <!-- REGULAR LINK -->
                      <li class="header1__nav-left-list-item">
                        <a href="{{ link.url }}" class="header1__nav-left-link">
                          {{ link.title | escape }}
                        </a>
                      </li>

                    {% endif %} 
                  {%- endfor -%}
                {%- endif -%}
              </ul>
            </nav>
          </div>

          <div class="col-4 col-md-4 d-flex align-items-center justify-content-center">
            <div class="header1__nav-logo-center-wrapper">
              <a href="/">
                {%- if section.settings.logo != blank -%}
                {%- assign image_size = section.settings.logo_width | append: 'x' -%}
                  <img
                    srcset="{{ section.settings.logo | img_url: image_size }} 1x, {{ section.settings.logo | img_url: image_size, scale: 2 }} 2x"
                    src="{{ section.settings.logo | img_url: image_size }}" loading="lazy" class="header__heading-logo"
                    width="{{ section.settings.logo.width }}" height="{{ section.settings.logo.height }}"
                    alt="{{ section.settings.logo.alt | default: shop.name | escape }}">
                {%- else -%}
                <h1 class="header1__nav-logo">
                  {{ shop.name }}
                </h1>
                {%- endif -%}
              </a>
            </div>
          </div>

          <div class="col-4 colmd-4 d-flex align-items-center justify-content-end">
            <nav class="header1__nav-right">
              <ul class="header1__nav-right-list d-flex align-items-center">
                {%- if section.settings.menu_2 != blank -%}
                  {%- for link in section.settings.menu_2.links -%}
                    {% if settings.mega_title == link.title %}
                      <!-- REGULAR LINK WITH A DROPDOWN MEGAMENU RIGHT SIDE -->
                      <li class="header1__nav-right-list-item header1__has-dropdown site-nav--is-megamenu">
                        <a href="" class="header1__nav-right-link header1__nav--link-has-dropdown">
                          {{ link.title }}
                        </a>
                        <!-- Megamenu START -->
                        <div class="nav-dropdown text-left megamenu">
                          <div class="page-width">
                            <div class="container">
                              <div class="row">
                                {% for childlink in link.links %}
                                <div class="col-sm-3 col-md-3 col-lg-3">
                                  <h5 class="megamenu__collection-heading py-3">
                                    <a href="#">{{ childlink.title }}</a>
                                  </h5>
                                  <ul class="megamenu__list text-left nav-dropdown__list">
                                    {% for grandchildlink in childlink.links %}
                                    <li class="nav-dropdown__list-item">
                                      <a href="{{ grandchildlink.url }}" class="nav-dropdown__link">
                                        {{ grandchildlink.title }}
                                      </a>
                                    </li>
                                    {%endfor%}
                                  </ul>
                                </div>
                                {% endfor %}
                              </div>
                            </div>
                          </div>
                        </div>
                        <!-- Megamenu END -->
                      </li>
                    {% elsif link.links != blank %}
                      <!-- REGULAR LINK WITH A DROPDOWN MENU RIGHT SIDE -->
                      <li class="header1__nav-right-list-item header1__has-dropdown">
                        <a href="" class="header1__nav-right-link header1__nav--link-has-dropdown">
                          {{ link.title }} <span class="dropdown-icon"><i class="bi bi-chevron-down"></i></span>
                        </a>
                        <!-- Dropdown Menu START -->
                        <ul class="nav-dropdown text-left nav-dropdown__list">
                          {% for childlink in link.links %}
                          <li class="nav-dropdown__list-item">
                            <a href="{{ childlink.url }}" class="nav-dropdown__link">{{ childlink.title }}</a>
                          </li>
                          {% endfor %}
                        </ul>
                        <!-- Dropdown Menu END -->
                      </li>
                    {% else %}
                      <!-- REGULAR LINK -->
                      <li class="header1__nav-right-list-item">
                        <a href="{{ link.url }}" class="header1__nav-right-link">
                          {{ link.title }}
                        </a>
                      </li>
                    {% endif %}
                  {%- endfor -%}
                {%- endif -%}

                <!-- MERCHANT CONTROLS -->
                
                <li class="header1__nav-right-list-item">
                  <button type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasTop" aria-controls="offcanvasTop">
                    <span class="header5__nav-right-search"><i class="bi bi-search"></i></span>
                  </button>
                </li>
                <li class="header1__nav-right-list-item">
                  {%- if shop.customer_accounts_enabled -%}
                  <a href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"
                    class="menu-drawer__account link link--text focus-inset" style="padding-bottom: 5px;">
                    {% render 'icon-account' %}
                    <span class="visually-hidden">
                      {%- liquid
                      if customer
                        echo 'customer.account_fallback' | t
                      else
                        echo 'customer.log_in' | t
                      endif
                      -%}
                    </span>
                  </a>
                  {%- endif -%} 
                </li>
                <li class="header1__nav-right-list-item">  
                  <a href="{{ routes.cart_url }}" class="header__icon header__icon--cart link link--text focus-inset"
                    id="cart-icon-bubble">
                    {%- liquid
                    if cart == empty
                    render 'icon-cart'
                    else
                    render 'icon-cart'
                    endif
                    -%}
                    <span class="visually-hidden">{{ 'templates.cart.cart' | t }}</span>
                    {%- if cart != empty -%}
                    <div class="cart-count-bubble">
                      {%- if cart.item_count < 100 -%} <span aria-hidden="true">{{ cart.item_count }}</span>
                        {%- endif -%}
                        <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                    </div>
                    {%- endif -%}
                  </a>


                </li>
                
              </ul>
            </nav>
          </div>
        </div>
      </div>
    </div>

    <!-- Mobile Start -->
    <div class="header1__mobile-nav-wrapper">
      <nav class="nav nav1 nav-m1 position">
        <div class="container">
          <div class="row d-flex align-items-center justify-content-space-between mobile-content">
